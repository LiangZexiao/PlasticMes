/******* Common ********/

BODY {
	FONT-SIZE: 9pt; 
	font-family:宋体;
	COLOR: #000000; 
	TEXT-DECORATION: none;
    background-color: #ffffff;
    Margin: 0; 
}

TD {
	FONT-SIZE: 9pt
}

image {
	border: 0
}

TH {
	FONT-SIZE: 9pt;
	font-weight:normal
}

HR {
	COLOR: #a5bbd1
}
.label
{
	 color:Blue;
}
/******* Link ********/

A:link {
	FONT-SIZE: 9pt; COLOR: #000000; TEXT-DECORATION: none
}
A:visited {
	FONT-SIZE: 9pt; COLOR: #333333; TEXT-DECORATION: none
}
A:hover {
	FONT-SIZE: 9pt; COLOR: #ff0000; TEXT-DECORATION: none
}

A.white:link {
	COLOR: #ffffff; 
	TEXT-DECORATION: none
}
A.white:visited {
	COLOR: #ffffff; 
	TEXT-DECORATION: none
}
A.white:hover {
	COLOR: #ffffff; 
	TEXT-DECORATION: underline
}

A.blue:link {
	COLOR: #055bae; 
	TEXT-DECORATION: none
}
A.blue:visited {
	COLOR: #055bae; 
	TEXT-DECORATION: none
}
A.blue:hover {
	COLOR: #055bae; 
	TEXT-DECORATION: underline
}


/******* Font ********/

.font_white {
	COLOR: #ffffff
}
.font_red {
	COLOR: #ff0000
}
.font_boldblue {
	COLOR: #055bae;
	font-weight:bold;
}
.font_gray {
	COLOR: #333333
}
.font_enl {
	COLOR: #ff0000;
	font-family:arial
}
/*******Button ********/

.btn1 {
	BORDER-RIGHT: #4b6f89 1px solid;
	BORDER-TOP: buttonhighlight 1px solid; 
	FONT: 12px 宋体; 
	BORDER-LEFT: buttonhighlight 1px solid;
	CURSOR: hand; 
	COLOR: #ffffff; 
	BORDER-BOTTOM: #4b6f89 1px solid; 
	HEIGHT: 21px; 
	BACKGROUND-COLOR: #739dbf; 
	TEXT-ALIGN: center; 
	TEXT-DECORATION: none
}
.button {
	BORDER-RIGHT: 1px solid; 
	BORDER-TOP: #cccccc 1px solid; 
	BORDER-LEFT: #cccccc 1px solid; 
	COLOR: #ffffff; 
	BORDER-BOTTOM: 1px solid; 
	BACKGROUND-COLOR: #0146C5;
	height:18px;
    width:60px
}
.btn_60 {
	BORDER-RIGHT: 1px solid; 
	BORDER-TOP: #cccccc 1px solid; 
	BORDER-LEFT: #cccccc 1px solid; 
	COLOR: #000000; 
	BORDER-BOTTOM: 1px solid; 
	BACKGROUND-COLOR: #d6d3ce;
	width:60px;
	height:18px
}
.txt {
	BORDER-RIGHT: 1px inset; 
	BORDER-TOP: 1px inset; 
	FONT-SIZE: 8pt; 
	VERTICAL-ALIGN: text-bottom; 
	BORDER-LEFT: 1px inset; 
	BORDER-BOTTOM: 1px inset; 
	FONT-FAMILY: Tahoma
}
.statxt {
	BORDER-RIGHT: #666666 1px solid; 
	BORDER-TOP: #666666 1px solid; 
	FONT-SIZE: 9pt; 
	BORDER-LEFT: #666666 1px solid; 
	BORDER-BOTTOM: #666666 1px solid; 
	HEIGHT: 15px; 
	BACKGROUND-COLOR: #f9fcff
}


/******* Input ********/

.textarea {
	height:50px
}
.multiple {
	height:100px
}
.colorbox {
	width:24px;
	height:24px
}



/******* Action ********/
.SmallButton {
	behavior:url("htc/SmallButton.htc");
	cursor:pointer
}

.gridview
{
 	 PADDING-TOP: 2.5px;
	 PADDING-BOTTOM: 2.5px;
	 PADDING-LEFT: 3px;
	 PADDING-RIGHT: 3px; 
}

.textbox 
{
	 Width:120px;
	 
}
.textboxodl
{
	width:120px;
	background-color:Yellow;
}
.txtlab
{
	COLOR:Black;
	font-weight:bold;
}
.dropdownlist
{
	Width:120px;
    margin-left: 0px;
}
.hidden
{
	display:none
}
/******* Table ********/
.hl {
	BACKGROUND-COLOR: #ededec
}
.sl {
	BACKGROUND-COLOR: #e0edf7
}



.section-content {
	width:99%;
	height:100%;
    valign:top;
	myalign:expression(this.align="center"); 
	padding: 8px;
	background-color: #F9F9F9;
	border: 1px solid #CCCCCC;
}


.tablelist {
	BORDER-RIGHT: #c1cdd8 1px solid; 
	PADDING-RIGHT: 4px; 
	BORDER-TOP: #c1cdd8 1px solid; 
	MARGIN-TOP: 0px; 
	PADDING-LEFT: 4px; 
	PADDING-BOTTOM: 2px; 
	MARGIN-LEFT: 0px; 
	BORDER-LEFT: #c1cdd8 1px solid; 
	WIDTH: 100%; 
	MARGIN-RIGHT: 0px; 
	PADDING-TOP: 4px; 
	BORDER-BOTTOM: #c1cdd8 1px solid; 
	BORDER-COLLAPSE: collapse; 
	TEXT-ALIGN: left; 
	empty-cells: show
}

.sort {
	COLOR: #000000
}
.toplinebg {
	PADDING-RIGHT: 4px;
	BORDER-TOP: #f5f5f5 1px solid; 
	PADDING-LEFT: 4px; 
	PADDING-BOTTOM: 2px; 
	PADDING-TOP: 4px; 
	color:#055bae; 
	BACKGROUND:url(../images/topline_bg.gif); 	
	height: 24px;
}

.tablefoot {
	WIDTH: 100%; 
	height: 24px;
	BORDER-TOP: #f5f5f5 1px solid; 
	PADDING-LEFT: 4px; 
	BACKGROUND:url(../images/btn_bg.gif); 
	BORDER-RIGHT: #c1cdd8 1px solid; 
	BORDER-TOP: #c1cdd8 1px solid; 
	BORDER-LEFT: #c1cdd8 1px solid; 
	BORDER-BOTTOM: #c1cdd8 1px solid; 
}
/*************************************
itemtable style start
*************************************/

table.itemtable {
	width: 100%;
	BACKGROUND-COLOR: #4A7594;
}

table.itemtable tbody tr th {
	color: Black;
	text-align: left;
	padding: 5px;
	BACKGROUND:url(../images/topline_bg.gif); 
}

table.itemtable tr{
	BACKGROUND-COLOR: #ffffff;
}

table.itemtable tfoot tr td, .toppager {
	padding-left: 5px;
	height: 24px;
	BACKGROUND:url(../images/btn_bg.gif); 
}


/*************************************
itemtable style end
*************************************/

/*************************************
texttable style start
*************************************/

table.texttable {
	width: 100%;	
	BACKGROUND-COLOR: #4A7594;
}
/********************

********************/
table.texttable th {
	BACKGROUND-COLOR: #EBEFF0;
	font-size:9pt

}

table.texttable tr{
	BACKGROUND-COLOR: #ffffff;
}

.field_table {
	BACKGROUND-COLOR: #EBEFF0;
	text-align:center;
}

/*************************************
texttable style end
*************************************/

/*************************************
buttontable style start
*************************************/

.buttontable {
	WIDTH: 100%; 
	border:1px;
	BORDER-left: #cccccc 1px solid; 
	BORDER-right: #cccccc 1px solid; 
	BORDER-top: #cccccc 1px solid; 
	BORDER-bottom: #cccccc 1px solid; 
	PADDING-RIGHT: 4px; 
	PADDING-LEFT: 4px; 
	PADDING-BOTTOM: 4px; 
	PADDING-TOP: 4px; 
	TEXT-ALIGN: right; 
    BACKGROUND-COLOR: #F9F9F9;
}

/*************************************
buttontable style end
*************************************/



/*************************************
htc
*************************************/
.toolbaricon {
	padding: 1px;
	margin: 1px;
	cursor: default;
	height: 16px;
	text-align: left;
	font-size: 9pt;
	behavior: url("images/toolbar.htc");
	cursor:default;
}

.inp_off {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; BACKGROUND: url(btn_blank2.gif); BORDER-LEFT: 0px; WIDTH: 70px; COLOR: #000000; BORDER-BOTTOM: 0px; HEIGHT: 22px
}
.inp_on {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; BACKGROUND: url(btn_on2.gif); BORDER-LEFT: 0px; WIDTH: 70px; COLOR: #000000; BORDER-BOTTOM: 0px; HEIGHT: 22px
}
.font_login {
	FONT-WEIGHT: bold; FILTER: DropShadow(color=white,OffX=1, OffY=1,Positive=1); COLOR: #003399; LINE-HEIGHT: 11pt; POSITION: relative
}
.font_gray {
	FILTER: DropShadow(color=white,OffX=1, OffY=1,Positive=1); COLOR: #666666; LINE-HEIGHT: 11pt; POSITION: relative
}
