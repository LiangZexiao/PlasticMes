BODY {
	FONT-SIZE: 12px; 
	BACKGROUND-IMAGE: url(bg.jpg); 
	COLOR: #003399
}
TD {
	FONT-SIZE: 12px; COLOR: #003399
}
TH {
	FONT-SIZE: 12px; COLOR: #003399
}
IMG {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; BORDER-LEFT: 0px; BORDER-BOTTOM: 0px
}
.INPUT {
	FONT-SIZE: 12px; WIDTH: 145px; FONT-FAMILY: "arail"
}
FRAMESET {
	BORDER-RIGHT: #d4d0c8 1px; BORDER-TOP: #d4d0c8 0px; BORDER-LEFT: #d4d0c8 0px; BORDER-BOTTOM: #d4d0c8 0px
}
SELECT {
	BORDER-RIGHT: #7f9faa 1px solid; BORDER-TOP: #7f9faa 1px solid; FONT-SIZE: 12px; BORDER-LEFT: #7f9faa 1px solid; BORDER-BOTTOM: #7f9faa 1px solid; FONT-FAMILY: "simsun"; BACKGROUND-COLOR: #d6e7ef
}
TEXTAREA {
	BORDER-RIGHT: #afc4d5 1px solid; BORDER-TOP: #afc4d5 1px solid; FONT-SIZE: 12px; BACKGROUND: #d6e7ef; BORDER-LEFT: #afc4d5 1px solid; COLOR: #000066; BORDER-BOTTOM: #afc4d5 1px solid
}
TABLE {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; FONT-SIZE: 9pt; BORDER-LEFT: 0px; BORDER-BOTTOM: 0px; FONT-FAMILY: ו,MS SONG,SimSun,tahoma,sans-serif
}
A {
	COLOR: #666666; TEXT-DECORATION: none
}
.inp_off {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; BACKGROUND: url(btn_blank.gif); BORDER-LEFT: 0px; WIDTH: 70px; COLOR: #000000; BORDER-BOTTOM: 0px; HEIGHT: 24px
}
.inp_on {
	BORDER-RIGHT: 0px; BORDER-TOP: 0px; BACKGROUND: url(btn_on.gif); BORDER-LEFT: 0px; WIDTH: 70px; COLOR: #000000; BORDER-BOTTOM: 0px; HEIGHT: 24px
}
.font_login {
	FONT-WEIGHT: bold; FILTER: DropShadow(color=white,OffX=1, OffY=1,Positive=1); COLOR: #003399; LINE-HEIGHT: 11pt; POSITION: relative
}
.font_gray {
	FILTER: DropShadow(color=white,OffX=1, OffY=1,Positive=1); COLOR: #666666; LINE-HEIGHT: 11pt; POSITION: relative
}
