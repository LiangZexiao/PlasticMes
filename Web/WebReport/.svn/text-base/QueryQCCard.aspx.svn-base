                                        onselectstart="return false;" style="width: 121px" type="text" />
                                <input id="txteTime" runat="server" class="textbox" maxlength="5" name="txtTime2"
                                    oncut="checkPaste()" ondrag="checkPaste()" onfocus="" onkeydown="keydown(this,'time')"
