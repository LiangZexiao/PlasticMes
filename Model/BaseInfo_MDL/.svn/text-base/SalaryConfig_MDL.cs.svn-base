using System;
using System.Collections.Generic;
using System.Text;

namespace Admin.Model.BaseInfo_MDL
{
    public class SalaryConfig_MDL
    {
        private int _ID;

        private string _WorkType;

        private string _WorkDesc;

        private string _Level;

        private string _LevelDesc;


        private string _MoneyPreHour;

        public SalaryConfig_MDL() { }
        public SalaryConfig_MDL(string vWorkType)
        {
          this._WorkType = vWorkType;
        }
        public SalaryConfig_MDL(int vID, string vWorkType, string vWorkDesc, string vLevel, string vLevelDesc, string vMoneyPreHour)
        {
            this._ID = vID;
            this._WorkType = vWorkType;
            this._WorkDesc = vWorkDesc;
            this._Level = vLevel;
            this._LevelDesc = vLevelDesc;
            this._MoneyPreHour = vMoneyPreHour;
        }


        public int ID
        {
            get { return _ID; }
            set { _ID = value; }
        }

        public string WorkType
        {
            get { return _WorkType; }
            set { _WorkType = value; }
        }

        public string WorkDesc
        {
            get { return _WorkDesc; }
            set { _WorkDesc = value; }
        }

        public string LevelDesc
        {
            get { return _LevelDesc; }
            set { _LevelDesc = value; }
        }
        public string Level
        {
            get { return _Level; }
            set { _Level = value; }
        }
        public string MoneyPreHour
        {
            get { return _MoneyPreHour; }
            set { _MoneyPreHour = value; }
        }
    }
}
